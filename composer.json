{
    "name": "spatie/tax-calculator",
    "description": "A set of interfaces and methods to clean up your application's tax calculations",
    "keywords": [
        "spatie",
        "tax-calculator"
    ],
    "homepage": "https://github.com/spatie/tax-calculator",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=7.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5|^9.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\TaxCalculator\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\TaxCalculator\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
